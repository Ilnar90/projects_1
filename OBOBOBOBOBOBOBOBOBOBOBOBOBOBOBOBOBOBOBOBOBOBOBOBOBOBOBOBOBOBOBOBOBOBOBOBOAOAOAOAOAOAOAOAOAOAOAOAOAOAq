[32m+[m[32m  end[m
[32m+[m
[32m+[m[32m  test "should create micropost" do[m
[32m+[m[32m    assert_difference('Micropost.count') do[m
[32m+[m[32m      post :create, micropost: {  }[m
[32m+[m[32m    end[m
[32m+[m
[32m+[m[32m    assert_redirected_to micropost_path(assigns(:micropost))[m
[32m+[m[32m  end[m
[32m+[m
