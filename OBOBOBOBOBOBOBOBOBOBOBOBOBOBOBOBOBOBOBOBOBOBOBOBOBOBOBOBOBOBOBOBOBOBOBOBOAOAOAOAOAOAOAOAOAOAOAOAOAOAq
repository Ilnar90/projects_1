[32m+[m[32mProjects1::Application.routes.draw do[m
[32m+[m[32m  # The priority is based upon order of creation:[m
[32m+[m[32m  # first created -> highest priority.[m
[32m+[m
[32m+[m[32m  # Sample of regular route:[m
[32m+[m[32m  #   match 'products/:id' => 'catalog#view'[m
[32m+[m[32m  # Keep in mind you can assign values other than :controller and :action[m
[32m+[m
[32m+[m[32m  # Sample of named route:[m
[32m+[m[32m  #   match 'products/:id/purchase' => 'catalog#purchase', :as => :purchase[m
[32m+[m[32m  # This route can be invoked with purchase_url(:id => product.id)[m
[32m+[m
[32m+[m[32m  # Sample resource route (maps HTTP verbs to controller actions automatically):[m
[32m+[m[32m  #   resources :products[m
[32m+[m
[32m+[m[32m  # Sample resource route with options:[m
[32m+[m[32m  #   resources :products do[m
[32m+[m[32m  #     member do[m
[32m+[m[32m  #       get 'short'[m
[32m+[m[32m  #       post 'toggle'[m
[32m+[m[32m  #     end[m
[32m+[m[32m  #[m
[32m+[m[32m  #     collection do[m
[32m+[m[32m  #       get 'sold'[m
[32m+[m[32m  #     end[m
[32m+[m[32m  #   end[m
[32m+[m
[32m+[m[32m  # Sample resource route with sub-resources:[m
[32m+[m[32m  #   resources :products do[m
[32m+[m[32m  #     resources :comments, :sales[m
[32m+[m[32m  #     resource :seller[m
[32m+[m[32m  #   end[m
[32m+[m
[32m+[m[32m  # Sample resource route with more complex sub-resources[m
[32m+[m[32m  #   resources :products do[m
[32m+[m[32m  #     resources :comments[m
[32m+[m[32m  #     resources :sales do[m
[32m+[m[32m  #       get 'recent', :on => :collection[m
[32m+[m[32m  #     end[m
[32m+[m[32m  #   end[m
[32m+[m
[32m+[m[32m  # Sample resource route within a namespace:[m
[32m+[m[32m  #   namespace :admin do[m
[32m+[m[32m  #     # Directs /admin/products/* to Admin::ProductsController[m
[32m+[m[32m  #     # (app/controllers/admin/products_controller.rb)[m
[32m+[m[32m  #     resources :products[m
[32m+[m[32m  #   end[m
[32m+[m
[32m+[m[32m  # You can have the root of your site routed with "root"[m
[32m+[m[32m  # just remember to delete public/index.html.[m
[32m+[m[32m  # root :to => 'welcome#index'[m
[32m+[m
[32m+[m[32m  # See how all your routes lay out with "rake routes"[m
[32m+[m
[32m+[m[32m  # This is a legacy wild controller route that's not recommended for RESTful applications.[m
[32m+[m[32m  # Note: This route will make all actions in every controller accessible via GET requests.[m
[32m+[m[32m  # match ':controller(/:action(/:id))(.:format)'[m
[32m+[m[32mend[m
