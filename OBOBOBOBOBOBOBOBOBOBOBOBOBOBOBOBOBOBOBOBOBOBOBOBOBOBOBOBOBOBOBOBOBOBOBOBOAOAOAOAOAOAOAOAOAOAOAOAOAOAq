[32m+[m[32mrequire File.expand_path('../boot', __FILE__)[m
[32m+[m
[32m+[m[32mrequire 'rails/all'[m
[32m+[m
[32m+[m[32mif defined?(Bundler)[m
[32m+[m[32m  # If you precompile assets before deploying to production, use this line[m
[32m+[m[32m  Bundler.require(*Rails.groups(:assets => %w(development test)))[m
[32m+[m[32m  # If you want your assets lazily compiled in production, use this line[m
[32m+[m[32m  # Bundler.require(:default, :assets, Rails.env)[m
[32m+[m[32mend[m
[32m+[m
[32m+[m[32mmodule Projects1[m
[32m+[m[32m  class Application < Rails::Application[m
[32m+[m[32m    # Settings in config/environments/* take precedence over those specified here.[m
[32m+[m[32m    # Application configuration should go into files in config/initializers[m
[32m+[m[32m    # -- all .rb files in that directory are automatically loaded.[m
[32m+[m
[32m+[m[32m    # Custom directories with classes and modules you want to be autoloadable.[m
[32m+[m[32m    # config.autoload_paths += %W(#{config.root}/extras)[m
[32m+[m
[32m+[m[32m    # Only load the plugins named here, in the order given (default is alphabetical).[m
[32m+[m[32m    # :all can be used as a placeholder for all plugins not explicitly named.[m
[32m+[m[32m    # config.plugins = [ :exception_notification, :ssl_requirement, :all ][m
[32m+[m
[32m+[m[32m    # Activate observers that should always be running.[m
[32m+[m[32m    # config.active_record.observers = :cacher, :garbage_collector, :forum_observer[m
[32m+[m
[32m+[m[32m    # Set Time.zone default to the specified zone and make Active Record auto-convert to this zone.[m
[32m+[m[32m    # Run "rake -D time" for a list of tasks for finding time zone names. Default is UTC.[m
[32m+[m[32m    # config.time_zone = 'Central Time (US & Canada)'[m
[32m+[m
[32m+[m[32m    # The default locale is :en and all translations from config/locales/*.rb,yml are auto loaded.[m
[32m+[m[32m    # config.i18n.load_path += Dir[Rails.root.join('my', 'locales', '*.{rb,yml}').to_s][m
[32m+[m[32m    # config.i18n.default_locale = :de[m
[32m+[m
[32m+[m[32m    # Configure the default encoding used in templates for Ruby 1.9.[m
[32m+[m[32m    config.encoding = "utf-8"[m
[32m+[m
[32m+[m[32m    # Configure sensitive parameters which will be filtered from the log file.[m
[32m+[m[32m    config.filter_parameters += [:password][m
[32m+[m
[32m+[m[32m    # Enable escaping HTML in JSON.[m
[32m+[m[32m    config.active_support.escape_html_entities_in_json = true[m
[32m+[m
[32m+[m[32m    # Use SQL instead of Active Record's schema dumper when creating the database.[m
[32m+[m[32m    # This is necessary if your schema can't be completely dumped by the schema dumper,[m
[32m+[m[32m    # like if you have constraints or database-specific column types[m
[32m+[m[32m    # config.active_record.schema_format = :sql[m
[32m+[m
[32m+[m[32m    # Enforce whitelist mode for mass assignment.[m
[32m+[m[32m    # This will create an empty whitelist of attributes available for mass-assignment for all models[m
[32m+[m[32m    # in your app. As such, your models will need to explicitly whitelist or blacklist accessible[m
[32m+[m[32m    # parameters by using an attr_accessible or attr_protected declaration.[m
[32m+[m[32m    config.active_record.whitelist_attributes = true[m
[32m+[m
