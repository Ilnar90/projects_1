[32m+[m[32m    config.assets.enabled = true[m
[32m+[m
[32m+[m[32m    # Version of your assets, change this if you want to expire all your assets[m
[32m+[m[32m    config.assets.version = '1.0'[m
[32m+[m[32m  end[m
